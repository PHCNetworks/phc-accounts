<p id="notice"><%= notice %></p>
<div class="card">
  <div class="card-header card-primary">
    <h4>Information Index</h4>
  </div>
  <div class="card-body">

    <table class="table table-striped ">
      <thead>
        <tr>
          <th>User ID</th>
          <th>Username</th>
          <th>First Name</th>
          <th>Last Name</th>
          <th>Email</th>
        </tr>
      </thead>
      <tbody>
        <% @admin_users.each do |phcaccounts_user| %>
          <tr>
            <td><%= phcaccounts_user.id %></td>
            <td><%= phcaccounts_user.username %></td>
            <td><%= phcaccounts_user.firstname %></td>
            <td><%= phcaccounts_user.lastname %></td>
            <td><%= phcaccounts_user.email %></td>
          </tr>
        <% end %>
      </tbody>
    </table>

  </div>
</div>
