<p id="notice"><%= notice %></p>
<div class="card">
  <div class="card-header card-primary">
  <h4>Information Index</h4>
  </div>
  <div class="card-body">

  <table class="table table-striped ">
    <thead>
      <tr>
        <th>User ID</th>
        <th>Username</th>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Email</th>
      </tr>
    </thead>
    <tbody>
      <% @admin_users.each do |phc_accounts_user_index| %>
        <tr>
          <td><%= phc_accounts_user_index.id %></td>
          <td><%= phc_accounts_user_index.username %></td>
          <td><%= phc_accounts_user_index.firstname %></td>
          <td><%= phc_accounts_user_index.lastname %></td>
          <td><%= phc_accounts_user_index.email %></td>
        </tr>
      <% end %>
    </tbody>
  </table>

  </div>
</div>
